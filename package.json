    "config": "^3.3.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "knex": "^0.21.16",
    "lru-cache": "^6.0.0",
    "objection": "^2.2.15",
    "pg": "^8.6.0",
    "pino": "^6.11.3",
    "pino-pretty": "^4.7.1",
    "redis": "^3.1.2",
    "redlock": "^4.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/bluebird": "^3.5.33",
    "@types/config": "^0.0.38",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/knex": "^0.16.1",
    "@types/lru-cache": "^5.1.0",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.1",
    "@types/pino": "^6.3.8",
    "@types/redis": "^2.8.28",
    "@types/redlock": "^4.0.1",
    "@types/sinon": "^10.0.0",
    "@types/supertest": "^2.0.11",
    "mocha": "^8.3.2",
